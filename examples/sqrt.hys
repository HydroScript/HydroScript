// defining sqrt function
sqrt = >- n {
    // returns 0 if n == 0
    n == 0 ? <- 0

    // computing
    x = n / 2
    x != y -? {
        y = x
        x = (y + n / y) / 2
    }
    
    // returning
    <- x
}

// testing
console.log(sqrt(2))
console.log(sqrt(5))
console.log(sqrt(12))
console.log(sqrt(179))
console.log(sqrt(3456))
console.log(sqrt(42563))
console.log(sqrt(123124562))